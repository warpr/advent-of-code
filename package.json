{
    "name": "advent-of-code",
    "version": "2022.0.1",
    "description": "Advent of Code",
    "lint-staged": {
        "*.php": [
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ],
        "*.js": [
            "prettier --write"
        ],
        "*.mjs": [
            "prettier --write"
        ]
    },
    "author": "Kuno Woudt <kuno@frob.nl>",
    "license": "copyleft-next-0.3.1",
    "repository": "https://github.com/warpr/advent-of-code.git",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@prettier/plugin-php": "^0.19.2",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.4",
        "prettier": "^2.8.0",
        "nodemon": "^2.0.20"
    }
}
